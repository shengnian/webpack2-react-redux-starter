{
  "name": "webpack2-react-redux-starter",
  "version": "0.0.2",
  "description": "A webpack2 react redux stater kit",
  "main": "index.js",
  "build": {
    "productName": "React Redux Electron Starter",
    "appId": "YourBundleId",
    "app-category-type": "public.app-category.yourCategory",
    "win": {
      "iconUrl": "",
      "setupIcon": ""
    },
    "mac": {
      "icon": "app_build/icon.icns"
    },
    "asar": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "electron": "better-npm-run electron",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "yarn run lint && yarn run test && yarn run clean && yarn run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "electron": {
      "command": "yarn run deploy:prod && electron electronIndex.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shengnian/webpack2-react-redux-starter.git"
  },
  "author": "C <c@shengnian.org> (http://shengnian.org)",
  "license": "MIT",
  "keywords": [
    "react",
    "webpack 2.x",
    "babel",
    "react component",
    "webpack2-react-redux-starter"
  ],
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "^0.0.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "codecov": "^1.0.1",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "debug": "^2.5.1",
    "electron-devtools-installer": "^2.1.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.4",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.1",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "yargs": "^6.5.0"
  },
  "dependencies": {
    "add-dom-event-listener": "^1.0.1",
    "animate.css": "^3.5.2",
    "animejs": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-runtime": "^6.20.0",
    "bootstrap": "twbs/bootstrap#v4-dev",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "ladda": "^1.0.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "shallowequal": "^0.2.2"
  }
}
